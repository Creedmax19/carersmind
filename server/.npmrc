# Set package-lock to version 2
package-lock=true
package-lock-only=false

# Save exact version in package.json
save-exact=true

# Set engine strict to true
engine-strict=true

# Set the package manager
engine-strict=true

# Set the registry (uncomment and modify if using a private registry)
# registry=https://registry.npmjs.org/

# Set the cache settings
cache=${HOME}/.npm-cache
prefer-offline=true

# Set the loglevel
loglevel=error

# Set the progress bar
progress=false

# Set the fund setting to false
fund=false

# Set the audit settings
audit=false

# Set the package lock settings
package-lock=true

# Set the save prefix
save-prefix=""

# Set the strict-ssl setting
strict-ssl=true

# Set the maxsockets setting
maxsockets=5

# Set the fetch-retries setting
fetch-retries=3

# Set the fetch-retry-mintimeout setting
fetch-retry-mintimeout=10000

# Set the fetch-retry-maxtimeout setting
fetch-retry-maxtimeout=60000

# Set the fetch-retry-factor setting
fetch-retry-factor=10

# Set the fetch-timeout setting
fetch-timeout=300000

